<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="236.0" prefWidth="666.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clienteescritorio.controladores.FXMLModuloSucursalController">
   <HBox alignment="CENTER_LEFT" spacing="10">
      <children>
         <Label text="busqueda" />
         <TextField />
         <Button mnemonicParsing="false" text="buscar" />
         <Button mnemonicParsing="false" text="cancelar" />
      </children>
   </HBox>
    
    <TableView fx:id="tableView" prefHeight="172.0" prefWidth="648.0">
        <columns>
            <TableColumn fx:id="idColumn" minWidth="7.0" prefWidth="22.0" text="ID" />
            <TableColumn fx:id="nombreColumn" prefWidth="54.0" text="Nombre" />
         <TableColumn prefWidth="75.0" text="Encargado" />
         <TableColumn prefWidth="124.0" text="Calle" />
         <TableColumn prefWidth="57.0" text="numero" />
         <TableColumn prefWidth="55.0" text="Colonia" />
         <TableColumn prefWidth="95.0" text="Codigo Postal" />
         <TableColumn prefWidth="55.0" text="Ciudad" />
         <TableColumn prefWidth="55.0" text="Latitud" />
         <TableColumn prefWidth="71.0" text="Longitud" />
            <!-- Add other columns as needed -->
        </columns>
    </TableView>
   <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="596.0" spacing="10.0">
      <children>
         <Button mnemonicParsing="false" text="Registrar" />
         <Button mnemonicParsing="false" text="Actualizar" />
         <Button mnemonicParsing="false" text="eliminar" />
      </children>
   </HBox>
    

    
</VBox>
